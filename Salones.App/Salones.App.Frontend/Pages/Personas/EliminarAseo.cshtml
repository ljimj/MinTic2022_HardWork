@page
@model Salones.App.Frontend.Pages.EliminarAseoModel
@{
}
<h1>Se va a eliminar el Persona Aseo</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombres</th>
            <th scope="col">Apellidos</th>
            <th scope="col">Tipo identificacion</th>
            <th scope="col">Numero de documento</th>
            <th scope="col">Edad</th>
            <th scope="col">Estado COVID</th>
            <th scope="col">Turno</th>
            <th scope="col">Sintomas</th>
            <th scope="col">Estado Covid</th>
            <th scope="col">Fecha Diagnostico</th>
            <th scope="col">Periodo Aislamiento </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td> @Model.personaAseo.nombres </td>
            <td> @Model.personaAseo.apellidos </td>
            <td> @Model.personaAseo.tipoIdentificacion </td>
            <td> @Model.personaAseo.identificacion </td>
            <td> @Model.personaAseo.edad </td>
            <td> @Model.personaAseo.estadoCovid </td>
            <td> @Model.personaAseo.turno </td>
            <td> </td>
            <td> </td>
            <td> </td>
            <td> </td>
        </tr>
    </tbody>
</table>
<form method="post">
    <input type="hidden" asp-for="personaAseo.id">
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Confirmar Eliminar</button>
        <a class="btn btn-primary table-tbn" asp-page="/Personas/ListAseo"> Volver </a>
    </div>
</form>